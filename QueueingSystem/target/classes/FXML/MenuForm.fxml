<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.MenuFormController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="440.0" layoutY="231.0" prefHeight="258.0" prefWidth="401.0" spacing="20.0">
         <children>
            <Button fx:id="autoModeButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onClickAutoMode" prefHeight="50.0" prefWidth="400.0" text="Автоматический режим">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button fx:id="stepModeButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onClickStepMode" prefHeight="50.0" prefWidth="400.0" text="Пошаговый режим">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button fx:id="settingsButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onClickSettingsButton" prefHeight="50.0" prefWidth="400.0" text="Настройки">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#onClickExitButton" prefHeight="50.0" prefWidth="400.0" text="Выход">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
